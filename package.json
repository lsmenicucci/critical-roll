{
  "name": "criticalroll",
  "description": "A non-comercial app used for helping remotly playing RPG with friends",
  "author": {
    "name": "Lucas Menicucci",
    "email": "lucamenimeni@gmail.com"
  },
  "version": "0.0.6",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.github.lsmenicucci.criticalroll",
    "productName": "Critical Roll",
    "win": {
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "deep-equal": "^2.0.3",
    "deep-object-diff": "^1.1.0",
    "dot-prop-immutable": "^2.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-redux": "^1.5.1",
    "electron-updater": "^4.3.1",
    "i": "^0.3.6",
    "npm": "^6.14.4",
    "polished": "^3.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-scroll-to-bottom": "^1.3.2",
    "react-spring": "^8.0.27",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.0",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "dev-electron": "wait-on http://localhost:3000 && electron .",
    "dev-electron-react": "cross-env BROWSER=none npm run start",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm build",
    "electron-pack": "npm run build && electron-builder build -w",
    "release": "build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.2",
    "@storybook/react": "^5.3.18",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.3",
    "electron-builder": "^22.6.1",
    "wait-on": "^4.0.1"
  }
}